<resources>
    <string name="app_name">BetterWeather</string>
    <string name="app_name_and_version" translatable="false">
		<![CDATA[<b>BetterWeather</b>&nbsp;<font color="#888888">v%1$s</font>]]>
	</string>
    <string name="extension_title">BetterWeather</string>
    <string name="extension_description">Viser up-to-date vejr information for din lokation eller for en valgt lokation.
        Drevet af Yahoo! Weather og OpenWeatherMap.
    </string>
    <string name="title_settings">BetterWeather Indstillinger</string>

    <!-- App Chooser -->
    <string name="title_apps">Apps</string>
    <string name="title_shortcuts">Genveje</string>

    <!-- Toasts -->
    <string name="toast_refreshing">Opdaterer BetterWeather&#8230;</string>

    <!-- Weather -->
    <string name="temperature_template" translatable="false">%1$s°</string>
    <string name="highlow_template" translatable="false">(%1$s/%2$s°)</string>
    <string name="weather_expanded_title_template" translatable="false">
        %1$s — %2$s
    </string>

    <string name="today_forecast_template">I dag: %1$s, %2$s/%3$s°</string>
    <string name="tomorrow_forecast_template">I morgen: %1$s, %2$s/%3$s°</string>

    <!--  Can customize the order of addresses. %1=small Location, %2=large Location
        For example,
            "%1$s, %2$s" ==> "New York, USA"
            "%2$s %1$s" ==> "Korea Seoul" (for asian users, this form with no comma is more natural)
     -->
    <string name="location_template">%1$s, %2$s</string>

    <!-- Normal western users have empty %4$s, ie. empty speed unit prefix.
        However asian users may have prefix defined in
        below weather_speed_units_display_prefix_names array.
    -->
    <string name="wind_details_template">Vind: %1$s %4$s%2$s %3$s</string>
    <string name="wind_chill_template">Føles som: %1$s°</string>
    <string name="humidity_template">Fugtighed: %1$s</string>

    <string name="status_none" translatable="false">––</string>

    <!-- Normal western users have *empty* speed unit prefix names(%4$s in above location_template).
            For example, "Wind: SW <empty>3 mph"
        However some asians may have unit prefix name.
            For example, "Wind: SW <HOURLY>3 MILE" with their language.
            HOURLY is defined in weather_speed_units_display_prefix_names.
    -->
    <string-array name="weather_speed_units_display_prefix_names">
        <item></item>
        <item></item>
        <item></item>
    </string-array>
    <!-- This array is just for displaying unit in <WIDGET>, not in setting menu.
        Asian users can have their own unit name with their own language.
        Normal western users may have same values with strings in pref menu.
    -->
    <string-array name="weather_speed_units_display_names">
        <item>mph</item>
        <item>km/h</item>
        <item>m/s</item>
    </string-array>

    <string name="shortcut_pref_help_text">Deaktiver "Opdater vejr ved berøring" for at bruge</string>

    <!-- About -->
