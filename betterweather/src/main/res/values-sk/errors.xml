<resources>
    <string name="error_status">--</string>
    <!-- Unknown error -->
    <string name="error_unknown">Došlo k neznámej chybe.</string>
    <string name="error_unknown_expandedBody">Prosím, skúste aktualizovať miniaplikáciu alebo zmeniť nastavenia. Aktualizácia bude vykonaná za niekoľko minút.</string>
    <!-- Couldn't get location error -->
    <string name="error_location">Nenájdená poloha.</string>
    <string name="error_location_expandedBody">Zdá sa, že nie je možné určiť vašu polohu. Prosím, skontrolujte nastavenie prístupu k polohe v nastaveniach Androidu a následne skúste vykonať aktualizáciu počasia.</string>
    <!-- No internet connection -->
    <string name="error_internet">Žiadne internetové pripojenie.</string>
    <string name="error_internet_expandedBody">Nie je možné sa pripojiť k internetu. Skontrolujte nastavenia a pokúste sa vykonať aktualizáciu.</string>
    <!-- API Key error -->
    <string name="error_api">Chyba API.</string>
    <string name="error_api_expandedBody">Služba nie je dostupná alebo váš API kľúč nie je správny. Skontrolujte váš kľúč a skúste to znova neskôr.</string>
</resources>
